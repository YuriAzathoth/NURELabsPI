<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="Lab02Frame">
		<id_arg>0</id_arg>
		<style>wxCAPTION|wxDEFAULT_FRAME_STYLE</style>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<rows>2</rows>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>6</cols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
							<label>Pen color</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxColourPickerCtrl" name="ID_COLOURPICKERCTRL1" variable="Color" member="yes">
							<colour>#000000</colour>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
							<label>Pen size</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxSlider" name="ID_SLIDER1" variable="SizeSlider" member="yes">
							<value>1</value>
							<min>1</min>
							<max>50</max>
							<size>135,20</size>
							<handler function="OnSizeSliderCmdSliderUpdated" entry="EVT_COMMAND_SLIDER_UPDATED" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="SizeText" member="yes">
							<label>1</label>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="ButtonClear" member="yes">
							<label>Clear</label>
							<handler function="OnButtonClearClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxPanel" name="ID_PANEL1" variable="Canvas" member="yes">
					<size>480,320</size>
					<bg>#FFFFFF</bg>
					<handler function="OnCanvasPaint" entry="EVT_PAINT" />
					<handler function="OnCanvasLeftDown" entry="EVT_LEFT_DOWN" />
					<handler function="OnCanvasLeftUp" entry="EVT_LEFT_UP" />
					<handler function="OnCanvasRightDown" entry="EVT_RIGHT_DOWN" />
					<handler function="OnCanvasRightUp" entry="EVT_RIGHT_UP" />
					<handler function="OnCanvasMouseMove" entry="EVT_MOTION" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
